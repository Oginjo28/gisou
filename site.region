<!doctype html>
<html data-wf-domain="gisou-collective.webflow.io" data-wf-page="{squarespace.page-id}" data-wf-site="gisou-collective" lang="en">
<head>
    <meta charset="utf-8" />
    <title>{website.metaTitleFormat}</title>
    <meta name="description" content="{website.metaDescription}" />
    <meta content="{website.metaTitleFormat}" property="og:title" />
    <meta content="{website.metaDescription}" property="og:description" />
    <meta content="{website.logoImageUrl}" property="og:image" />
    <meta content="{website.metaTitleFormat}" property="twitter:title" />
    <meta content="{website.metaDescription}" property="twitter:description" />
    <meta content="{website.logoImageUrl}" property="twitter:image" />
    <meta property="og:type" content="website" />
    <meta content="summary_large_image" name="twitter:card" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="Squarespace" name="generator" />
    <link href="https://fonts.googleapis.com" rel="preconnect" />
    <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin="anonymous" />
    {squarespace-headers}
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
    <script type="text/javascript">
        WebFont.load({
            google: {
                families: [
                    "Playfair Display:regular,500,600,700,800,900",
                    "Fraunces:100,200,300,regular,500,600,700,800,900,100italic,200italic,300italic,italic,500italic,600italic,700italic,800italic,900italic:latin,latin-ext,vietnamese",
                    "Noto Serif Display:100,200,300,regular,500,600,700,800,900",
                    "Inter:100,200,300,regular,500,600,700,800,900",
                ],
            },
        });
    </script>
    <script type="text/javascript">
        !(function (o, c) {
            var n = c.documentElement,
                t = " w-mod-";
            (n.className += t + "js"), ("ontouchstart" in o || (o.DocumentTouch && c instanceof DocumentTouch)) && (n.className += t + "touch");
        })(window, document);
    </script>
</head>
<body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <div class="page-wrapper">
        <!-- Navigation -->
        <nav class="navbar">
            <div class="padding-global">
                <div class="container-large">
                    <div class="nav-content">
                        <div class="nav-links-wrapper">
                            <a data-w-id="571c6f19-26aa-91ed-c434-cf11dac0c7f2" href="#" class="nav-link is-menu w-inline-block">
                                <div class="simple-dot light"></div>
                                <div>menu</div>
                            </a>
                            <squarespace:navigation navigationId="mainNav" template="navigation" />
                        </div>
                        <div class="nav-brand-cell">
                            <a href="/" class="nav-brand w-inline-block">
                                <div class="nav-brand-logo w-embed">
                                    <squarespace:block-field id="siteLogo" columns="12" />
                                </div>
                            </a>
                        </div>
                        <div class="nav-links-wrapper right">
                            <squarespace:navigation navigationId="secondaryNav" template="navigation" />
                        </div>
                    </div>
                </div>
            </div>
            <!-- Mobile Menu Overlay -->
            <squarespace:block-field id="mobileMenuOverlay" columns="12" />
        </nav>

    <div class="page-divider top-divider"></div>

    <!-- // page image or divider -->
    {.section collection}
      {.background-source-video?}
      <div id="hero">
        <div class="page-image" data-collection-id="{collection.id}" data-edit-main-image="Banner" data-annotation-alignment="bottom left">
          <div class="image-wrapper">
            {@|apply sqs-video-background.block}
          </div>
          <div class="wrapper">
            <h1 class="page-title">{collection.title}</h1>
            <div class="page-description">{collection.description}</div>
          </div>
        </div>
        <!-- Main Content -->
        <main id="main" class="main-wrapper" role="main" data-content-field="main-content">
            <div style="padding: 40px; text-align: center; background: linear-gradient(135deg, #667eea 0%, #764ba2 100%); color: white; margin: 20px; border-radius: 10px;">
                <h1>ðŸŽ¨ Gisou Collective Template - UPDATED</h1>
                <p>Your custom Squarespace template is loading! Build: 2:15 PM</p>
                <div style="background: rgba(255,255,255,0.1); padding: 20px; margin: 20px 0; border-radius: 8px;">
                    <h2>Template Status: âœ… Active & CSS Fixed</h2>
                    <p>Import errors should be resolved now. Cache refreshed.</p>
                </div>
            </div>
            {squarespace.main-content}
        </main>

        <!-- Footer -->
        <footer class="site-footer">
            <div class="padding-global padding-section-large">
                <div class="container-large align-x-center">
                    <div class="horizontal-line-wrapper for-footer"><div class="line horizontal"></div></div>
                    <div class="footer-main-box">
                        <squarespace:block-field id="footerContent" columns="12" />
                    </div>
                </div>
            </div>
        </footer>
    </div>

    {squarespace-footers}
</body>
</html>
